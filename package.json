{
  "name": "ziyume.com",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:website": "turbo run website#build",
    "dev": "turbo run dev",
    "dev:website": "turbo run website#dev",
    "start:website": "turbo run website:start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "typecheck:website": "turbo run website#typecheck"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.4",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20"
  }
}
